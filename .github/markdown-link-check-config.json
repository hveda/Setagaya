{
  "timeout": "20s",
  "retryOn429": true,
  "retryCount": 3,
  "fallbackHTTPStatus": 200,
  "aliveStatusCodes": [
    200,
    206
  ],
  "ignorePatterns": [
    {
      "pattern": "^http://localhost"
    },
    {
      "pattern": "^https://localhost"
    },
    {
      "pattern": "^http://127.0.0.1"
    },
    {
      "pattern": "^https://127.0.0.1"
    },
    {
      "pattern": "^https://docs.example.com"
    },
    {
      "pattern": "^http://storage:8080"
    },
    {
      "pattern": "^http://db:"
    },
    {
      "pattern": "^https://your-org.com"
    },
    {
      "pattern": "http://docs.grafana.org/v3.0/plugins/installation/"
    },
    {
      "pattern": "^https://github.com/hveda/Setagaya/compare/"
    },
    {
      "pattern": "^https://github.com/hveda/Setagaya/issues/"
    },
    {
      "pattern": "kubernetes/"
    },
    {
      "pattern": ".github/workflows/"
    }
  ],
  "httpHeaders": [
    {
      "urls": [
        "https://github.com"
      ],
      "headers": {
        "Accept-Encoding": "zstd, br, gzip, deflate"
      }
    }
  ]
}
